<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<bs3:grid
        xsi:schemaLocation="https://causeway.apache.org/applib/layout/component https://causeway.apache.org/applib/layout/component/component.xsd https://causeway.apache.org/applib/layout/grid/bootstrap3 https://causeway.apache.org/applib/layout/grid/bootstrap3/bootstrap3.xsd"
        xmlns:c="https://causeway.apache.org/applib/layout/component"
        xmlns:bs3="https://causeway.apache.org/applib/layout/grid/bootstrap3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <bs3:row>
        <bs3:col span="12" unreferencedActions="true">
            <!--<cpt:domainObject bookmarking="AS_ROOT"/>-->
        </bs3:col>
    </bs3:row>
    <bs3:row>
        <bs3:col span="6">
            <bs3:row>
                <bs3:col span="12">
                                <c:fieldSet name="Datos del Reclamo">
                                    <c:property id="nroReclamo" namedEscaped="true"/>
                                    <c:property id="estado" namedEscaped="true"/>
                                    <c:property id="usuario" namedEscaped="true"/>
                                    <c:property id="direccion" namedEscaped="true"/>
                                    <c:property id="fecha" namedEscaped="true"/>
                                    <c:property id="tipoReclamo" namedEscaped="true"/>
                                    <c:property id="descripcion" namedEscaped="true" multiLine="8" hidden="ALL_TABLES"/>
                                    <c:property id="fechaCierre" namedEscaped="true"/>
                                    <c:property id="observacion" namedEscaped="true" multiLine="8" hidden="ALL_TABLES"/>
                                </c:fieldSet>
                </bs3:col>
            </bs3:row>
        </bs3:col>
    </bs3:row>
    <bs3:row>
        <bs3:col span="12">
            <bs3:tabGroup>
                <bs3:tab name="Other">
                    <bs3:row>
                        <bs3:col span="12">
                            <c:fieldSet name="Other" unreferencedProperties="true"/>
                        </bs3:col>
                    </bs3:row>
                </bs3:tab>
                <bs3:tab name="Metadata">
                    <bs3:row>
                        <bs3:col span="0">
                            <c:fieldSet name="Metadata"/>
                        </bs3:col>
                    </bs3:row>
                </bs3:tab>
            </bs3:tabGroup>
        </bs3:col>
        <bs3:col span="12">
            <bs3:tabGroup unreferencedCollections="true"/>
        </bs3:col>
    </bs3:row>
</bs3:grid>
